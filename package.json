{
  "name": "dice-game",
  "version": "1.0.0",
  "private": true,
  "description": "一款通过投掷骰子进行对战的小游戏",
  "templateInfo": {
    "name": "vue3-pinia",
    "typescript": false,
    "css": "none"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:weapp:preview": "taro preview --type weapp",
    "build:weapp:upload": "taro upload --type weapp",
    "build:weapp:upload2": "taro upload --type weapp --projectPath dist/xxx",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "vitest run"
  },
  "taroConfig": {
    "version": "1.0.0",
    "desc": "CI自动上传"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "softeng-underdog",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "@tarojs/components": "^3.6.17",
    "@tarojs/helper": "^3.6.17",
    "@tarojs/plugin-framework-vue3": "^3.6.17",
    "@tarojs/plugin-platform-alipay": "^3.6.17",
    "@tarojs/plugin-platform-h5": "^3.6.17",
    "@tarojs/plugin-platform-jd": "^3.6.17",
    "@tarojs/plugin-platform-qq": "^3.6.17",
    "@tarojs/plugin-platform-swan": "^3.6.17",
    "@tarojs/plugin-platform-tt": "^3.6.17",
    "@tarojs/plugin-platform-weapp": "^3.6.17",
    "@tarojs/runtime": "^3.6.17",
    "@tarojs/shared": "^3.6.17",
    "@tarojs/taro": "^3.6.17",
    "pinia": "^2.0.10",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@tarojs/cli": "^3.6.17",
    "@tarojs/plugin-mini-ci": "^3.6.17",
    "@tarojs/taro-loader": "^3.6.17",
    "@tarojs/webpack5-runner": "^3.6.17",
    "@types/node": "^20.8.2",
    "@types/webpack-env": "^1.18.2",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-preset-taro": "^3.6.17",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-taro": "^3.6.17",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.31",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vue-loader": "^17.0.0",
    "webpack": "^5.78.0"
  }
}
